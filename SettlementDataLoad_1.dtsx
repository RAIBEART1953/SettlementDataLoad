      <DTS:ObjectData>
        <pipeline
          version="1">
          <components>
            <component
              refId="Package\Scorecard\Scorecard"
              componentClassID="Attunity.SSISODBCSrc"
              description="ODBC Source"
              name="Scorecard"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.String"
                  description="The name of the table to be fetched."
                  expressionType="Notify"
                  name="TableName"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT DISTINCT cad.client_account_id,
                cad.business_dt,
                cast(substring(cad.tradelineid, 4, 50) as int)      as tradeline_id,
                cad.tradelineid,
                cad.programid,
                cad.creditor_accepted_ratio,
                cad.creditor,
                cad.eligibility,
                cad.creditor_score,
                cad.state_of_residency,
                cast(cad.avg_monthly_payment as int)                as avg_monthly_payment,
                cad.account_delinquency,
                cast((cad.creditor_accepted_ratio * 100) as int)    as pct_avg_settlement,
                cad.fee_funding_pct,
                cad.fee,
                substring(cad.account_status, 1, 25)                as account_status,
                cad.enrolled_debt,
                assu.account_total_score                            as totalscore,
                assu.account_total_score_rank                       as rank,
                assu.account_program_rank                           as accountrepeatcountbyindex,
                NULL                                                as calculatedcreditscore,
                assu.account_index_value                            as concatenatedindex,
                getdate()                                           as createdat,
                getdate()                                           as updatedat,
                cad.account_balance                                 as balance,
                TRIM((CAST(ROUND(cad.creditor_accepted_ratio * 100, 0) as varchar(4)) || ' pct.' || ' over ' ||
                      substring(cad.accepted_creditor_terms, 1, 10) || ' ' ||
                      substring(cad.accepted_payment, 1, 10)))      AS creditor_terms,
                cad.fund_accumulated                                as endofcurrentmonthfundaccumulation,
                cad.last_worked_on                                  as lastworkedon,
                tl.id                                               as sf_tl_id,
                cad.cft_funds,
                cad.pct_funds_available,
                cad.prospect_uuid,
                'Beyond'                                            as firm_name,
                pg.client_first_name__c + ' ' + client_last_name__c AS full_name,
                0                                                   as draft_amount,
                pg.createddate                                      as enrolled_dt,
                0                                                   as previousSettlements
FROM setl.client_account_data cad
         JOIN setl.account_score acsc
              ON acsc.client_account_id = cad.client_account_id AND acsc.business_dt = cad.business_dt AND
                 acsc.run_number = cad.run_number
         JOIN setl.account_score_summary assu
              on cad.client_account_id = assu.client_account_id AND assu.business_dt = cad.business_dt AND
                 cad.run_number = assu.run_number
         JOIN datalake_sf.sf_nu_dse_tradeline_c_src tl ON tl.name = cad.tradelineid
         JOIN datalake_sf.sf_nu_dse_program_c_src pg on pg.id = tl.nu_dse__program__c
WHERE cad.business_dt = (SELECT max(business_dt) FROM setl.processing_calendar)
  AND cad.run_number = (SELECT max(run_number)
                        FROM setl.processing_calendar
                        WHERE business_dt = (SELECT max(business_dt) FROM setl.processing_calendar))
ORDER BY assu.account_total_score desc, assu.account_total_score_rank;</property>
