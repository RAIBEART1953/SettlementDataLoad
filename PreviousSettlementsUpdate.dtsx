                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT
TL_Name
, CASE when sum(IsSettling) &gt; 0 then 1 else 0 end as HasSettlements
FROM (
         select p.name as TL_Name
              , case when t.nu_dse__current_stage__c != 'New' then 1 else 0 end as IsSettling

         from datalake_sf.sf_nu_dse_tradeline_c_src t
    inner join datalake_sf.sf_nu_dse_program_c_src p on t.nu_dse__program__c = p.id
     ) dtData
group by TL_Name</property>
